@model BoardPreview

<div class="card list-group mt-2">
    <a asp-page="/Board/Index" asp-route-boardId="@Model.BoardId"
       class="list-group-item list-group-item-action bg-primary text-white">@Model.BoardName</a>

    @foreach (var post in Model.PostList)
    {
        <a asp-page="/Board/Detail" asp-route-postNumber="@post.PostNumber" class="list-group-item list-group-item-action">@post.Title</a>
    }
</div>